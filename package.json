{
	"name": "sap-json-schemas",
	"displayName": "SAP JSON Schemas (UN-OFFICIAL)",
	"description": "JSON Schemas of SAP",
	"publisher": "TheoSun",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"jsonValidation": [
			{
				"fileMatch": "xs-app.json",
				"url": "./schemas/xs-app/xs-app-schema.json"
			},
			{
				"fileMatch": "xs-security.json",
				"url": "./schemas/xs-security/xs-security-schema.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Soontao/sap-json-schemas"
	}
}